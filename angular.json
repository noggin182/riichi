{
    "version": 1,
    "projects": {
        "scorer": {
            "projectType": "application",
            "schematics": {
                "@nrwl/angular:component": {
                    "style": "scss"
                }
            },
            "root": "apps/scorer",
            "sourceRoot": "apps/scorer/src",
            "prefix": "scorer",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/apps/scorer",
                        "index": "apps/scorer/src/index.html",
                        "main": "apps/scorer/src/main.ts",
                        "polyfills": "apps/scorer/src/polyfills.ts",
                        "tsConfig": "apps/scorer/tsconfig.app.json",
                        "aot": false,
                        "assets": [
                            "apps/scorer/src/favicon.ico",
                            "apps/scorer/src/assets"
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "apps/scorer/src"
                            ]
                        },
                        "styles": [
                            "apps/scorer/src/styles.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/scorer/src/environments/environment.ts",
                                    "with": "apps/scorer/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "scorer:build",
                        "port": 4300
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "scorer:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "scorer:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "apps/scorer/tsconfig.app.json",
                            "apps/scorer/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "!apps/scorer/**"
                        ]
                    }
                }
            }
        },
        "common": {
            "root": "libs/common",
            "sourceRoot": "libs/common/src",
            "projectType": "library",
            "schematics": {},
            "architect": {
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "libs/common/tsconfig.lib.json",
                            "libs/common/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "!libs/common/**"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "libs/common/jest.config.js",
                        "tsConfig": "libs/common/tsconfig.spec.json"
                    }
                }
            }
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/angular"
    },
    "schematics": {
        "@nrwl/angular:application": {
            "unitTestRunner": "jest",
            "e2eTestRunner": "cypress"
        },
        "@nrwl/angular:library": {
            "unitTestRunner": "jest"
        }
    },
    "defaultProject": "scorer"
}